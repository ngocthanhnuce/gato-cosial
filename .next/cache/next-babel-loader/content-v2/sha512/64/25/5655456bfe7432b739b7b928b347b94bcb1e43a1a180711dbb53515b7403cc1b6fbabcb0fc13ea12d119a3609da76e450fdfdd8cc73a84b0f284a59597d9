         checked={showWork}\r\n                onChange={() => {\r\n                  setShowWork(!showWork);\r\n                  handleChangeShow();\r\n                }}\r\n              />\r\n            </div>\r\n          </Grid.Column>\r\n        </Grid.Row>\r\n        <Grid.Row>\r\n          <Grid.Column width={12}>\r\n            <Form.Input\r\n              name=\"address\"\r\n              label=\"Address\"\r\n              value={address}\r\n              onChange={handleChange}\r\n              placeholder=\"address\"\r\n              icon=\"point\"\r\n            />\r\n          </Grid.Column>\r\n          <Grid.Column width={4}>\r\n            <div style={{ paddingTop: \"30px\" }}>\r\n              <Checkbox\r\n                label=\"Ẩn hoặc hiện\"\r\n                toggle\r\n                checked={!showAddress}\r\n                onChange={() => {\r\n                  setShowAddress(!showAddress);\r\n                  handleChangeShow();\r\n                }}\r\n              />\r\n            </div>\r\n          </Grid.Column>\r\n        </Grid.Row>\r\n        <Grid.Row>\r\n          <Grid.Column width={12}>\r\n            <Form.Input\r\n              name=\"relationship\"\r\n              label=\"Relationship\"\r\n              value={relationship}\r\n              onChange={handleChange}\r\n              placeholder=\"relationship\"\r\n              icon=\"heart\"\r\n            />\r\n          </Grid.Column>\r\n          <Grid.Column width={4}>\r\n            <div style={{ paddingTop: \"30px\" }}>\r\n              <Checkbox\r\n                label=\"Ẩn hoặc hiện\"\r\n                toggle\r\n                checked={!showRelationship}\r\n                onChange={() => {\r\n                  setShowRelationship(!showRelationship);\r\n                  handleChangeShow();\r\n                }}\r\n              />\r\n            </div>\r\n          </Grid.Column>\r\n        </Grid.Row>\r\n        <Grid.Row>\r\n          <Grid.Column width={12}>\r\n            <Form.Input\r\n              name=\"education\"\r\n              label=\"Education\"\r\n              value={education}\r\n              onChange={handleChange}\r\n              placeholder=\"education\"\r\n              icon=\"student\"\r\n            />\r\n          </Grid.Column>\r\n          <Grid.Column width={4}>\r\n            <div style={{ paddingTop: \"30px\" }}>\r\n              <Checkbox\r\n                label=\"Ẩn hoặc hiện\"\r\n                toggle\r\n                checked={!showEducation}\r\n                onChange={() => {\r\n                  setShowEducation(!showEducation);\r\n                  handleChangeShow();\r\n                }}\r\n              />\r\n            </div>\r\n          </Grid.Column>\r\n        </Grid.Row>\r\n        <Grid.Row>\r\n          <Grid.Column width={12}>\r\n            <Form.Input\r\n              name=\"birthday\"\r\n              label=\"Birthday\"\r\n              value={birthday}\r\n              onChange={handleChange}\r\n              placeholder=\"birthday\"\r\n              icon=\"birthday cake\"\r\n            />\r\n          </Grid.Column>\r\n          <Grid.Column width={4}>\r\n            <div style={{ paddingTop: \"30px\" }}>\r\n              <Checkbox\r\n                label=\"Ẩn hoặc hiện\"\r\n                toggle\r\n                checked={!showBirthday}\r\n                onChange={() => {\r\n                  setShowBirthday(!showBirthday);\r\n                  handleChangeShow();\r\n                }}\r\n              />\r\n            </div>\r\n          </Grid.Column>\r\n        </Grid.Row>\r\n      </Grid>\r\n      <div style={{ paddingTop: \"20px\" }}>\r\n        <Button\r\n          content=\"Add Social Links\"\r\n          color=\"red\"\r\n          icon=\"rss\"\r\n          type=\"button\"\r\n          onClick={() => setShowSocialLinks(!showSocialLinks)}\r\n        />\r\n      </div>\r\n      {showSocialLinks && (\r\n        <>\r\n          <Divider />\r\n       