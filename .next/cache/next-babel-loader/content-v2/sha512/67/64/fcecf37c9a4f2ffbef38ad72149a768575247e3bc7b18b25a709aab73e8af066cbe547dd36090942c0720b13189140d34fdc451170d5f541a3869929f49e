s/profileActions\";\r\nimport uploadPic from \"../../utils/uploadPicToCloudinary\";\r\nimport ImageDropDiv from \"../Common/ImageDropDiv\";\r\n\r\nfunction ProfileHeader({\r\n  profile,\r\n  ownAccount,\r\n  loggedUserFollowStats,\r\n  setUserFollowStats,\r\n}) {\r\n  const [loading, setLoading] = useState(false);\r\n  const [media, setMedia] = useState(null);\r\n  const [mediaPreview, setMediaPreview] = useState(null);\r\n  const [updatePic, setUpdatePic] = useState({\r\n    profilePicUrl: profile.user.profilePicUrl,\r\n  });\r\n  const inputRef = useRef();\r\n  const [errorMsg, setErrorMsg] = useState(null);\r\n  const [highlighted, setHighlighted] = useState(false);\r\n\r\n  const isFollowing =\r\n    loggedUserFollowStats.following.length > 0 &&\r\n    loggedUserFollowStats.following.filter(\r\n      (following) => following.user === profile.user._id\r\n    ).length > 0;\r\n\r\n  const handleChange = (e) => {\r\n    const { name, value, files } = e.target;\r\n\r\n    if (name === \"media\") {\r\n      setMedia(files[0]);\r\n      setMediaPreview(URL.createObjectURL(files[0]));\r\n    }\r\n    setUpdatePic((prev) => ({ ...prev, [name]: value }));\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <Segment>\r\n        <Grid stackable>\r\n          <Grid.Column width={11}>\r\n            <Grid.Row>\r\n              <Header\r\n                as=\"h2\"\r\n                content={profile.user.name}\r\n                style={{ marginBottom: \"5px\" }}\r\n              />\r\n            </Grid.Row>\r\n\r\n            <Grid.Row stretched>\r\n              {profile.bio}\r\n              <Divider hidden />\r\n            </Grid.Row>\r\n\r\n            <Grid.Row>\r\n              {profile.social ? (\r\n                <List>\r\n                  <List.Item>\r\n                    <List.Icon name=\"mail\" />\r\n                    <List.Content content={profile.user.email} />\r\n                  </List.Item>\r\n\r\n                  {profile.social.facebook && (\r\n                    <div\r\n                      style={{ cursor: \"pointer\" }}\r\n                      onClick={() => window.open(`${profile.social.facebook}`)}\r\n                    >\r\n                      <List.Item>\r\n                        <List.Icon name=\"facebook\" color=\"blue\" />\r\n                        <List.Content\r\n                          style={{ color: \"blue\" }}\r\n                          content={profile.social.facebook}\r\n                        />\r\n                      </List.Item>\r\n                    </div>\r\n                  )}\r\n\r\n                  {profile.social.instagram && (\r\n                    <div\r\n                      style={{ cursor: \"pointer\" }}\r\n                      onClick={() => window.open(`${profile.social.instagram}`)}\r\n                    >\r\n                      <List.Item>\r\n                        <List.Icon name=\"instagram\" color=\"red\" />\r\n                        <List.Content\r\n                          style={{ color: \"blue\" }}\r\n                          content={profile.social.instagram}\r\n                        />\r\n                      </List.Item>\r\n                    </div>\r\n                  )}\r\n\r\n                  {profile.social.youtube && (\r\n                    <div\r\n                      style={{ cursor: \"pointer\" }}\r\n                      onClick={() => window.open(`${profile.social.youtube}`)}\r\n                    >\r\n                      <List.Item>\r\n                        <List.Icon name=\"youtube\" color=\"red\" />\r\n                        <List.Content\r\n                          style={{ color: \"blue\" }}\r\n                          content={profile.social.youtube}\r\n                        />\r\n                      </List.Item>\r\n                    </div>\r\n                  )}\r\n\r\n                  {profile.social.twitter && (\r\n                    <div\r\n                      style={{ cursor: \"pointer\" }}\r\n                      onClick={() => window.open(`${profile.social.twitter}`)}\r\n                    >\r\n                      <List.Item>\r\n                        <List.Icon name=\"twitter\" color=\"blue\" />\r\n                        <List.Content\r\n                          style={{ color: \"blue\" }}\r\n                          content={profile.social.twitter}\r\n                        />\r\n                      </List.Item>\r\n                    </div>\r\n                  )}\r\n                </List>\r\n              ) : (\r\n                <>No Social Media Links </>\r\n              )}\r\n            </Grid.Row>\r\n          </Grid.Column>\r\n\r\n          <Grid.Column width={5} stretched style={{ textAlign: \"center\" }}>\r\n            <Grid.Row>\r\n              <Image size=\"large\" avatar src={profile.user.profilePicUrl} />\r\n            </Grid.Row>\r\n            <Grid.Row style={{ paddingTop: \"10px\" }}>\r\n              <Form\r\n                error={errorMsg !== null}\r\n                loading={loading}\r\n                onSubmit={async (e) => {\r\n                  e.preventDefault();\r\n                  setLoading(true);\r\n\r\n                  let profilePicUrl;\r\n\r\n                  if (media !== null) {\r\n                    profilePicUrl = await uploadPic(media);\r\n                  }\r\n\r\n                  if (media !== null && !profilePicUrl) {\r\n                    setLoading(false);\r\n                    return setErrorMsg(\"Error Uploading Image\");\r\n                  }\r\n\r\n                  await updateAvatar(setLoading, setErrorMsg, profilePicUrl);\r\n                }}\r\n              >\r\n                <Message\r\n                  onDismiss={() => setErrorMsg(false)}\r\n                  error\r\n                  content={errorMsg}\r\n                  attached\r\n                  header=\"Oops!\"\r\n                />\r\n\r\n                <ImageDropDiv\r\n                  inputRef={inputRef}\r\n                  highlighted={highlighted}\r\n                  setHighlighted={setHighlighted}\r\n                  handleChange={handleChange}\r\n                  mediaPreview={mediaPreview}\r\n                  setMediaPreview={setMediaPreview}\r\n                  setMedia={setMedia}\r\n                  profilePicUrl={profile.profilePicUrl}\r\n                />\r\n                <Button\r\n                  color=\"blue\"\r\n                  icon=\"pencil alternate\"\r\n                  disabled={profile.bio === \"\" || loading}\r\n                  content=\"Submit\"\r\n                  type=\"submit\"\r\n                />\r\n              </Form>\r\n              <Button\r\n                basic\r\n                color=\"teal\"\r\n                content=\"Thay avatar\"\r\n                icon=\"image outline\"\r\n              />\r\n            </Grid.Row>\r\n            <br />\r\n\r\n            {!ownAccount && (\r\n              <Button\r\n                compact\r\n                loading={loading}\r\n                disabled={loading}\r\n                content={isFollowing ? \"Following\" : \"Follow\"}\r\n                icon={isFollowing ? \"check circle\" : \"add user\"}\r\n                color={isFollowing ? \"instagram\" : \"twitter\"}\r\n                onClick={async () => {\r\n                  setLoading(true);\r\n                  isFollowing\r\n                    ? await unfollowUser(profile.user._id, setUserFollowStats)\r\n                    : await followUser(profile.user._id, setUserFollowStats);\r\n                  setLoading(false);\r\n                }}\r\n              />\r\n            )}\r\n          </Grid.Column>\r\n        </Grid>\r\n      </Segment>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default ProfileHeader;\r\n"]},"metadata":{},"sourceType":"module"}